<Window x:Class="Pl.windows.Order.ThankYou"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pl.windows.Order"
        mc:Ignorable="d"
        Title="ThankYou" Height="450" Width="800"
        DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/windows/Order/image (27).png"/>
        </Grid.Background>
        <Label Content="{Binding CotumerName}" HorizontalAlignment="Center" FontWeight="Bold" Foreground="White" FontSize="17" Margin="0,18,0,0" VerticalAlignment="Top" Height="139" Width="780"/>

    </Grid>
</Window>
